{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "cap:sync": "vite build && npx cap sync android",
    "cap:open": "npx cap open android",
    "cap:copy": "npx cap copy android",
    "cap:build": "vite build && npx cap sync android && echo 'Open Android Studio to build APK: npx cap open android'"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
